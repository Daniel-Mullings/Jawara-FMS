{
  "hosting": {
    "redirects": [
      {
          "source": "/index.html",
          "destination": "/homepage.html",
          "type": 301
      },
      {
          "source": "/",
          "destination": "/homepage.html",
          "type": 301
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}